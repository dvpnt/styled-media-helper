{
  "name": "styled-media-helper",
  "version": "2.0.2",
  "description": "helper for media query",
  "main": "./dist/index.js",
  "scripts": {
    "lint": "eslint ./",
    "test": "tap test/*.js --bail --coverage --coverage-report=text --100 --reporter specy",
    "clean": "rm -rf ./dist/*",
    "build-cjs": "babel src -d dist/cjs --verbose",
    "build-esm": "NODE_ENV=esm babel src -d dist/esm --verbose",
    "build": "npm run clean && npm run build-esm && npm run build-cjs",
    "prepare": "npm run build"
  },
  "author": "Maxim Shtyr <sh.maxim97@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dvpnt/styled-media-helper.git"
  },
  "keywords": [
    "breakpoint",
    "breakpoints",
    "css-in-js",
    "css-in-react",
    "javascript",
    "media",
    "media-queries",
    "media-query",
    "react",
    "styled",
    "styled-breakpoints",
    "styled-components"
  ],
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.6",
    "@babel/preset-env": "7.9.6",
    "babel-plugin-styled-components": "1.10.7",
    "eslint": "6.8.0",
    "eslint-config-dvpnt": "4.0.3",
    "eslint-plugin-import": "^2.20.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "styled-components": "4.1.3",
    "tap": "14.10.7"
  },
  "peerDependencies": {
    "styled-components": "5"
  }
}
